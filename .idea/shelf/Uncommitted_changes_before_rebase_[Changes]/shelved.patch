+#  希尔排序 时间复杂度O(n^2)，空间复杂度O(1)
+def shell_sort(arr):
+    n = len(arr)
+    gap = n // 2  # 初始间隔为数组长度的一半
+
+    while gap > 0:
+        for i in range(gap, n):
+            cur = arr[i]
+            j = i
+            while j >= gap and arr[j - gap] > cur:
+                arr[j] = arr[j - gap]
+                j -= gap
+            arr[j] = cur
+        gap //= 2  # 缩小间隔
+
+    return arr
+
+# 示例用法
+arr = [3, 44, 38, 5, 47, 15, 36, 26, 27, 2, 46, 4, 19, 50, 48]
+if __name__ == "__main__":
+    sorted_arr = shell_sort(arr)
+    print(sorted_arr)

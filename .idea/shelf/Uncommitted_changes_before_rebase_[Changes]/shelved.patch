+import os
+import string
+
+import pytest
+from shared_test_lib.utils.random_util import RandomUtil
+
+from pytest_yaochn_ui_base.yc_ui_context import YCUIContext
+from tests.collect.collect_base_test import CollectBaseTest
+
+
+class TestCrfs(CollectBaseTest):
+
+    test_data_path = os.path.join(os.path.dirname(__file__), 'gloden_data')
+    protocol_file_path = os.path.join(test_data_path, 'protocol_C0009.xlsx')
+    external_data_path = os.path.join(test_data_path, 'external_data.xlsx')
+
+    download_path = os.path.join(CollectBaseTest.DOWNLOADS_PATH, 'TestCrfs')
+
+    @pytest.fixture(autouse=True, scope='class')
+    def setup_teardown_class(self, predefined_study_crf_ui):
+        cur_study = predefined_study_crf_ui
+        self.YC_CONTEXT.organization(cur_study.organization).study(cur_study).env(self.ENVS.PROD)
+        yield
+
+    @pytest.fixture(scope='function')
+    def setup_study_config(self, yc_ui_context: YCUIContext):
+        self.YC_CONTEXT.user(self.DBD_USER['user']).role(self.DBD_USER['role'])
+        self.page = yc_ui_context.user_page(self.cur_user)
+        self.lab_page = self.pages.LabPage(self.page, self.study, "PROD", self.cur_role)
+        self.study_config_page = self.pages.StudyConfigPage(self.page, self.study, self.cur_role)
+        self.basics_page = self.pages.BasicsPage(self.page, self.study, self.cur_role)
+        self.crf_page = self.pages.CRFPage(self.page, self.study, self.cur_role)
+        yield
+
+    @pytest.fixture(scope='function')
+    def setup_lab_page(self, yc_ui_context: YCUIContext):
+        self.YC_CONTEXT.user(self.CDM_USER['user']).role(self.CDM_USER['role'])
+        self.page = yc_ui_context.user_page(self.cur_user)
+        self.lab_page = self.pages.LabPage(self.page, self.study, "PROD", self.cur_role)
+        yield
+
+    def test_study_info(self, setup_study_config):
+        # set study info
+        self.basics_page.open()
+        self.basics_page.external_lab_config()
+        self.basics_page.external_lab_config(is_enabled=False)
+        self.basics_page.field_path_config()
+        self.basics_page.external_data(external_data_path=self.external_data_path)
+        self.basics_page.crf_object_oid_prefix()
+
+    def test_create_crf_from_template(self, setup_study_config):
+        # set study config
+        self.study_config_page.open()
+        # self.study_config_page.subject_key_config()
+        self.study_config_page.subject_status_config()
+        self.study_config_page.deploy_to_environments(['UAT', "PROD"])
+
+        # set crf
+        self.crf_page.open()
+        self.crf_page.create_crf_from_template(construct_spec=self.protocol_file_path)
+        self.crf_page.crf_copy()
+        self.crf_page.crf_edit()
+        self.crf_page.crf_delete()
+        self.crf_page.crf_detail_page.deploy()
+        self.crf_page.access_first_crf_detail()
+        self.crf_page.crf_detail_page.packed_snapshot()
+        self.crf_page.crf_detail_page.publish()
+        self.crf_page.crf_detail_page.deploy()
+        self.crf_page.crf_detail_page.set_live()
+
+    def test_create_crf_from_blank(self, setup_study_config):
+        # set study config
+        self.study_config_page.open()
+        # self.study_config_page.subject_key_config()
+        self.study_config_page.subject_status_config()
+        self.study_config_page.deploy_to_environments(['UAT', "PROD"])
+
+        # set crf

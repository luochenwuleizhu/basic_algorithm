+#  插入排序 时间复杂度O(n^2)，空间复杂度O(1)
+arr = [3, 44, 38, 5, 47, 15, 36, 26, 27, 2, 46, 4, 19, 50, 48]
+def insertion_sort(arr):
+    for i in range(len(arr)):
+        cur = arr[i]
+        pre = i-1
+        # 将比 key 大的元素向右移动一位
+        while pre>=0 and arr[pre]>cur:
+            arr[pre+1]=arr[pre]
+            pre-=1
+        # 插入元素到正确位置
+        arr[pre+1]=cur
+    return arr
+if __name__=="__main__":
+    print(insertion_sort(arr))
